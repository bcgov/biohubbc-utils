{
  "name": "sims-utils",
  "version": "0.0.0",
  "description": "Template utility scripts for SIMS",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/biohubbc.git"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "master-build-validation-moose-srb": "ts-node ./observations/master-files/Moose/SRB/Moose_Aerial_StratifiedRandomBlock_Recruit_Comp_Survey_2.0_Validation_Config_Builder.ts",
    "master-transform-moose-srb": "ts-node ./observations/master-files/Moose/SRB/moose_srb_schema.ts",

    "master-build-validation-elk-srb": "ts-node ./observations/master-files/Elk/SRB/Elk_Aerial_SRB_Recruit_Comp_Survey_2.0_Validation_Config_Builder.ts",
    "master-transform-elk-srb": "ts-node ./observations/master-files/Elk/SRB/elk_srb_schema.ts",

    "build-validation-moose-general": "ts-node ./observations/validation/builder/Moose_Aerial_General_Recruit_comp_Survey_2.0_Validation_Config_Builder.ts",
    "build-validation-moose-srb": "ts-node ./observations/validation/builder/Moose_Aerial_StratifiedRandomBlock_Recruit_Comp_Survey_2.0_Validation_Config_Builder.ts",
    "build-validation-moose-transect": "ts-node ./observations/validation/builder/Moose_Aerial_Transect_Distance_Sampling_Survey_2.0_Validation_Config_Builder.ts",
    "build-validation-sheep": "ts-node ./observations/validation/builder/Sheep_2.0_Validation_Config_Builder.ts",
    "build-validation-goat": "ts-node ./observations/validation/builder/Goat_2.0_Validation_Config_Builder.ts",
    "build-validation-caribou": "ts-node ./observations/validation/builder/Caribou_1.0_Validation_Config_Builder.ts",
    "build-validation-elk-general": "ts-node ./observations/validation/builder/Elk_Aerial_General_Recruit_Comp_Survey_2.0_Validation_Config_Builder.ts",
    "build-validation-elk-srb": "ts-node ./observations/validation/builder/Elk_Aerial_SRB_Recruit_Comp_Survey_2.0_Validation_Config_Builder.ts",
    "build-transformation": "ts-node ./transformation/transformation-config-builder.ts",
    "transform-schema": "ts-node ./observations/transform/transform.ts",
    "transform-sheep": "ts-node ./observations/transform/schema/sheep_total_count_schema.ts",
    "transform-elk-srb": "ts-node ./observations/transform/schema/elk_srb_schema.ts",
    "transform-moose-srb": "ts-node ./observations/transform/schema/moose_srb_schema.ts",
    "transform-elk-general": "ts-node ./observations/transform/schema/elk_general_schema.ts",
    "transform-caribou": "ts-node ./observations/transform/schema/caribou_total_count_schema.ts",
    "transform-goat": "ts-node ./observations/transform/schema/goat_total_count_schema.ts",
    "transform-moose-general": "ts-node ./observations/transform/schema/moose_general_schema.ts",
    "build-summary-moose": "ts-node ./summary/builder/Moose_Summary_Config_Builder.ts",
    "build-summary-caribou": "ts-node ./summary/builder/Caribou_Summary_Config_Builder.ts",
    "build-summary-elk": "ts-node ./summary/builder/Elk_Summary_Config_Builder.ts",
    "build-summary-sheep": "ts-node ./summary/builder/Sheep_Summary_Config_Builder.ts",
    "build-summary-goat": "ts-node ./summary/builder/Goat_Summary_Config_Builder.ts",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format-fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss}\""
  },
  "devDependencies": {
    "@types/chai": "~4.2.22",
    "@types/fast-json-patch": "~1.1.5",
    "@types/mocha": "~9.0.0",
    "@types/node": "^18.15.3",
    "@types/sinon": "~10.0.4",
    "@types/sinon-chai": "~3.2.5",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "ajv": "~8.12.0",
    "chai": "~4.3.4",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-prettier": "~3.3.1",
    "nodemon": "~2.0.14",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "~2.3.4",
    "sinon": "~11.1.2",
    "sinon-chai": "~3.7.0",
    "ts-node": "~10.4.0",
    "typescript": "~4.1.6"
  },
  "dependencies": {
    "fast-json-patch": "~3.1.1",
    "fs": "^0.0.1-security",
    "jsonpath-plus": "~7.2.0",
    "ts-node": "~10.4.0",
    "typescript": "~4.1.6",
    "xlsx": "~0.18.5"
  }
}
