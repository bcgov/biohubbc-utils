{
  "name": "biohubbc-utils",
  "version": "0.0.0",
  "description": "Template utility scripts for SIMS",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/biohubbc-utils.git"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "caribou-observations": "ts-node ./src/observations/caribou",
    "caribou-summary": "ts-node ./src/summary/caribou",
    "elk-observations": "ts-node ./src/observations/elk",
    "elk-summary": "ts-node ./src/summary/elk",
    "goat-observations": "ts-node ./src/observations/goat",
    "goat-summary": "ts-node ./src/summary/goat",
    "moose-observations": "ts-node ./src/observations/moose",
    "moose-summary": "ts-node ./src/summary/moose",
    "sheep-observations": "ts-node ./src/observations/sheep",
    "sheep-summary": "ts-node ./src/summary/sheep",
    "observations": "ts-node ./src/observations",
    "summary": "ts-node ./src/summary",
    "all": "npm-run-all -l -s observations summary",
    "join": "ts-node ./src/join",
    "clean-observations": "rimraf -g \"./src/observations/**/output/*.{json,sql,xlsx,csv}\"",
    "clean-summary": "rimraf -g \"./src/summary/**/output/*.{json,sql,xlsx,csv}\"",
    "clean": "npm-run-all -l -s clean-observations clean-summary",
    "test": "ts-node ./src/test",
    "test-clean": "rimraf -g \"./src/**/test-run/*.{json,sql,xlsx,csv}\"",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format-fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "fix": "npm-run-all -l -s lint-fix format-fix"
  },
  "dependencies": {
    "dayjs": "^1.11.8",
    "fast-json-patch": "^3.1.1",
    "jsonpath-plus": "^7.2.0",
    "lodash": "^4.17.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.6",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "devDependencies": {
    "@types/fast-json-patch": "^1.1.5",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "ajv": "^8.12.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "rimraf": "^5.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.6"
  }
}
