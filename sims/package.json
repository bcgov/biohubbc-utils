{
  "name": "biohubbc-utils",
  "version": "0.0.0",
  "description": "Template utility scripts for SIMS",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/biohubbc-utils.git"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "caribou-observations": "ts-node ./sims/observations/caribou",
    "caribou-summary": "ts-node ./sims/summary/caribou",
    "elk-observations": "ts-node ./sims/observations/elk",
    "elk-summary": "ts-node ./sims/summary/elk",
    "goat-observations": "ts-node ./sims/observations/goat",
    "goat-summary": "ts-node ./sims/summary/goat",
    "moose-observations": "ts-node ./sims/observations/moose",
    "moose-summary": "ts-node ./sims/summary/moose",
    "sheep-observations": "ts-node ./sims/observations/sheep",
    "sheep-summary": "ts-node ./sims/summary/sheep",
    "observations": "ts-node ./sims/observations",
    "summary": "ts-node ./sims/summary",
    "all": "npm-run-all -l -s clean observations summary",
    "clean": "rimraf -g \"./sims/**/output/*.{json,sql}\"",
    "test": "ts-node ./sims/test",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format-fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "fix": "npm-run-all -l -s lint-fix format-fix"
  },
  "dependencies": {
    "fast-json-patch": "^3.1.1",
    "jsonpath-plus": "^7.2.0",
    "lodash": "^4.17.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.6",
    "xlsx": "^0.18.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/fast-json-patch": "^1.1.5",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.3",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "ajv": "^8.12.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "rimraf": "^5.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.6"
  }
}
